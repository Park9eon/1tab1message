{
  "name": "1Tab1Message",
  "version": "0.0.1-beta",
  "description": "탭을 열면 메시지가 나옵니다.",
  "homepage_url": "https://park9eon.com",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon@128.png",
    "16": "icon@16.png",
    "32": "icon@32.png",
    "48": "icon@48.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "chrome_style": true
  }
}
